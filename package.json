{
  "name": "vue-template-babel-compiler",
  "version": "0.6.0-0",
  "description": "Post compiler for Vue template render functions to support ES features with Babel",
  "main": "lib/index.js",
  "author": "JuniorTour",
  "license": "MIT",
  "files": [
    "lib/index.js",
    "updateVueTemplateBabelCompiler.sh"
  ],
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "test": " npm run build && jest",
    "inspectTest": "npm run build && node --inspect-brk ./node_modules/jest/bin/jest.js",
    "prepublishOnly": "jest && npm run build"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "devDependencies": {
    "@babel/preset-env": "^7.14.7",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "jest": "^24.1.0",
    "np": "^7.5.0",
    "rollup": "^2.53.0",
    "vue": "^2.6.0",
    "vue-template-compiler": "^2.6.0"
  },
  "dependencies": {
    "@babel/core": "^7.14.3",
    "@babel/plugin-proposal-optional-chaining": "^7.14.2",
    "@babel/plugin-transform-arrow-functions": "^7.14.5",
    "@babel/plugin-transform-block-scoping": "^7.14.5",
    "@babel/plugin-transform-destructuring": "^7.14.5",
    "@babel/plugin-transform-parameters": "^7.14.5",
    "@babel/plugin-transform-spread": "^7.14.5",
    "@babel/types": "^7.14.5"
  },
  "keywords": [
    "vuejs",
    "babel",
    "sfc",
    "vue-template",
    "vue-template-babel"
  ],
  "repository": "https://github.com/JuniorTour/vue-template-babel-compiler"
}
